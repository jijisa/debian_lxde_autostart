#!/usr/bin/xdotool
# tab name: pvdi5,harden2,durant2,myshell4

sleep 3

# durant2
key --delay $1 ctrl+Page_Up
sleep 1
type --delay $1 pvdi
key --delay $1 Return
sleep 1

# harden2
key --delay $1 ctrl+Page_Up
sleep 1
type --delay $1 harden
key --delay $1 Return
sleep 1

# pvdi5
key --delay $1 ctrl+Page_Up
sleep 1
type --delay $1 pvdi
key --delay $1 Return
sleep 1

# move to the right bottom - screen resolution: 1920x1080
search --name pvdi5
windowmove 1250 510
